{
  "name": "@agentset/cloudflare-tools",
  "version": "0.1.0",
  "description": "TypeScript client library for AgentSet Cloudflare Worker integration (AI Search + AI Gateway)",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "agentset",
    "cloudflare",
    "ai-search",
    "ai-gateway",
    "typescript"
  ],
  "author": "AgentSet Team",
  "license": "MIT",
  "devDependencies": {
    "@agentset/eslint-config": "workspace:*",
    "@agentset/prettier-config": "workspace:*",
    "@agentset/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsup": "^8.3.5",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "prettier": "@agentset/prettier-config"
}
