{
  "$schema": "https://turborepo.org/schema.json",
  "ui": "tui",
  "tasks": {
    "topo": {
      "dependsOn": [
        "^topo"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".cache/tsbuildinfo.json",
        "dist/**"
      ]
    },
    "dev": {
      "dependsOn": [
        "^dev"
      ],
      "cache": false,
      "persistent": false
    },
    "format": {
      "outputs": [
        ".cache/.prettiercache"
      ],
      "outputLogs": "new-only"
    },
    "lint": {
      "dependsOn": [
        "^topo",
        "^build"
      ],
      "outputs": [
        ".cache/.eslintcache"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "^topo",
        "^build"
      ],
      "outputs": [
        ".cache/tsbuildinfo.json"
      ]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "push": {
      "cache": false,
      "interactive": true
    },
    "jobs": {
      "persistent": true,
      "cache": false
    },
    "studio": {
      "cache": false,
      "persistent": true
    },
    "ui-add": {
      "cache": false,
      "interactive": true
    }
  },
  "globalEnv": [
    "NEXT_PUBLIC_APP_NAME",
    "NEXT_PUBLIC_APP_SHORT_DOMAIN",
    "VERCEL_PROJECT_ID",
    "VERCEL_TEAM_ID",
    "VERCEL_API_TOKEN",
    "DATABASE_URL",
    "DIRECT_URL",
    "BETTER_AUTH_SECRET",
    "BETTER_AUTH_URL",
    "RESEND_API_KEY",
    "QSTASH_URL",
    "QSTASH_TOKEN",
    "QSTASH_CURRENT_SIGNING_KEY",
    "QSTASH_NEXT_SIGNING_KEY",
    "GITHUB_CLIENT_ID",
    "GITHUB_CLIENT_SECRET",
    "GOOGLE_CLIENT_ID",
    "GOOGLE_CLIENT_SECRET",
    "REDIS_URL",
    "REDIS_TOKEN",
    "PORT",
    "STRIPE_API_KEY",
    "STRIPE_WEBHOOK_SECRET",
    "NEXT_PUBLIC_STRIPE_PUBLIC_KEY",
    "DISCORD_HOOK_ALERTS",
    "DISCORD_HOOK_CRON",
    "DISCORD_HOOK_SUBSCRIBERS",
    "DISCORD_HOOK_ERRORS",
    "DEFAULT_PINECONE_API_KEY",
    "DEFAULT_PINECONE_HOST",
    "SECONDARY_PINECONE_API_KEY",
    "SECONDARY_PINECONE_HOST",
    "DEFAULT_TURBOPUFFER_API_KEY",
    "CLOUDFLARE_ACCOUNT_ID",
    "CLOUDFLARE_API_TOKEN",
    "DEFAULT_CLOUDFLARE_ENDPOINT",
    "CF_SEARCH_ENDPOINT",
    "AZURE_SEARCH_URL",
    "AZURE_SEARCH_INDEX",
    "AZURE_SEARCH_KEY",
    "DEFAULT_COHERE_API_KEY",
    "DEFAULT_ZEROENTROPY_API_KEY",
    "PARTITION_API_URL",
    "PARTITION_API_KEY",
    "S3_ACCESS_KEY",
    "S3_SECRET_KEY",
    "S3_ENDPOINT",
    "S3_BUCKET",
    "ASSETS_S3_ACCESS_KEY",
    "ASSETS_S3_SECRET_KEY",
    "ASSETS_S3_ENDPOINT",
    "ASSETS_S3_BUCKET",
    "ASSETS_S3_URL",
    "TRIGGER_SECRET_KEY",
    "NEXT_PUBLIC_POSTHOG_KEY"
  ],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "CI",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "npm_lifecycle_event"
  ]
}